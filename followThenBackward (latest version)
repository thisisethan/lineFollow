#include <time.h>

int main()
{
	int msec = 0;
	clock_t start = clock(?), diff;
	while (digital(8) != 1 && msec <= 5000)
	{
		if (analog(6) > 512)
		{
			motor(0, 20);
			motor(3, 100);
			
		}
		
		else	
		{
				motor(0, 100);
				motor(3, 20);
		}
		diff = clock() - start;
		msec = diff * 1000 / CLOCKS_PER_SEC;
	}

	return 0;
	
}

