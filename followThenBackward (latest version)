#include <time.h>

int main()
{
	long sec = 0;
	clock_t start = clock(), diff;
	while (sec <= 4)
	{
		if (analog(6) < 900)
		{
			motor(0, 20);
			motor(3, 100);
			
		}
		
		else	
		{
				motor(0, 100);
				motor(3, 20);
		}
		diff = clock() - start;
		sec = diff * 10 / CLOCKS_PER_SEC;
	printf("seconds_running: %li\n", sec);
		}
		//{
		//potato 
		//}
		
	while (digital(8) != 1){}
	motor(0, -100);
	motor(3, -100);
	msleep(12500);
	return 0;
	
}
