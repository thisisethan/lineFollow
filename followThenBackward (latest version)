int main()
{
	//we need to figure out where to put this msleep: other ideas= use a for loop
msleep(15000000);
{
	while (digital(8) != 1)
	{
		if (analog(6) > 512)
		{
			motor(0, 20);
			motor(3, 100);
			
		}
		
		else	
		{
				motor(0, 100);
				motor(3, 20);
		}
	
	}

	motor(0, -100);
	motor(3, -100);
	msleep(12500);
	motor(0, -100);
	motor(3, -100);

}
	return 0;
}

