int main()
{
	while (digital(10) != 1)
	{
		if (analog(0) > 512)
		{
			motor(0,20);
			motor(3,100);
			
		}
		
		else
			
		{
			motor(0,100);
			motor(3,20);
			
		}
	
	}
	
	ao();
	
	return 0;
	
		{
	motor(0, 95);
	motor(1, 100);
	while (digital(8) !=1)
	
	motor(0, -95);
	motor(1, -100);
	msleep(5000);
	
	return 0;	
}

}
